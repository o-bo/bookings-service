{
  "name": "bookings-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start": "npm run build && node build/index.js",
    "build": "rimraf ./build && tsc",
    "start:dev": "nodemon",
    "prestart": "npm run knex:migrate:latest",
    "test": "jest",
    "pretest": "NODE_ENV=test npm run knex:migrate:latest",
    "knex:migrate:make": "knex --knexfile src/db/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/db/knexfile.ts migrate:rollback"
  },
  "dependencies": {
    "express": "~4.16.1",
    "install": "^0.13.0",
    "knex": "^0.95.11",
    "morgan": "~1.9.1",
    "npm": "^8.1.0",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
