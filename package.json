{
  "name": "bookings-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "start": "npm run build && node build/index.js",
    "build": "rimraf ./coverage && rimraf ./build && tsc",
    "start:dev": "nodemon",
    "prestart:dev": "npm run knex:migrate:latest",
    "prestart": "npm run knex:migrate:latest",
    "test": "NODE_ENV=test jest --testPathIgnorePatterns=\"e2e.test.(j|t)s\" --watchAll",
    "e2eTests": "NODE_ENV=test npm run pretest && jest --forceExit --runInBand -- e2e.test.ts",
    "cucumber-check": "node ./node_modules/@cucumber/cucumber/bin/cucumber-js src/__TESTS__/acceptance/features/**/*.feature --dry-run --require env/set-environment-variables.ts --require src/__TESTS__/acceptance/world/custom-world.ts --require src/__TESTS__/acceptance/step-definitions/**/*.ts --require src/__TESTS__/acceptance/hooks/**/*.ts  --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\" --format summary --format progress --format progress-bar  --publish-quiet",
    "cucumber": "node ./node_modules/@cucumber/cucumber/bin/cucumber-js src/__TESTS__/acceptance/features/**/*.feature --require env/set-environment-variables.ts --require src/__TESTS__/acceptance/world/custom-world.ts --require src/__TESTS__/acceptance/step-definitions/**/*.ts --require src/__TESTS__/acceptance/hooks/**/*.ts  --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\"  --format html:src/__TESTS__/acceptance/reports/report.html --format summary --format @cucumber/pretty-formatter --format cucumber-console-formatter --publish-quiet",
    "steps-usage": "node ./node_modules/cucumber/bin/cucumber-js src/__TESTS__/acceptance/features/**/*.feature --no-color --logLevel=error --require src/__TESTS__/acceptance/step-definitions/**/*.ts --require src/__TESTS__/acceptance/hooks/**/*.ts  --require-module ts-node/register --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\" --format usage --dry-run",
    "pretest": "NODE_ENV=test npm run knex:migrate:latest",
    "knex:migrate:make": "knex --knexfile src/bounded-contexts/bookings/infrastructure/spi/repositories/postgres/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "knex --knexfile src/bounded-contexts/bookings/infrastructure/spi/repositories/postgres/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/bounded-contexts/bookings/infrastructure/spi/repositories/postgres/knexfile.ts migrate:rollback"
  },
  "dependencies": {
    "express": "~4.16.1",
    "install": "^0.13.0",
    "knex": "^0.95.11",
    "morgan": "~1.9.1",
    "npm": "^8.1.0",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "shallow-equal-object": "^1.1.1",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@cucumber/html-formatter": "^18.0.0",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/pg": "^8.6.1",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "cucumber-console-formatter": "^1.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.2"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "setupFiles": [],
    "coverageDirectory": "coverage",
    "coverageProvider": "v8"
  }
}
