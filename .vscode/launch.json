  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/www",
      "env": {
        "NODE_ENV": "local"
      }
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Tests",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "env": {
        "NODE_ENV": "test"
      }
    }
  ]
}